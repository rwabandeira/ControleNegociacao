program ControleNegociacao;

uses
  Forms,
  Menu in 'Tela\Menu.pas' {FrMenu},
  _Conexao in 'Conexao\_Conexao.pas',
  CrudCadastro in 'Tela\CrudCadastro.pas' {FrCrudCadastro},
  Produtores in 'Tela\Produtores.pas' {FrProdutores},
  _Produtor in 'Banco\_Produtor.pas',
  PesquisarProdutores in 'Tela\PesquisarProdutores.pas' {FrPesquisarProdutores},
  _Biblioteca in '_Biblioteca.pas';

{$R *.res}

begin
  Application.Initialize;
  Application.MainFormOnTaskbar := True;
  Application.CreateForm(TFrMenu, FrMenu);
  Application.CreateForm(TFrCrudCadastro, FrCrudCadastro);
  Application.CreateForm(TFrProdutores, FrProdutores);
  Application.CreateForm(TFrPesquisarProdutores, FrPesquisarProdutores);
  Application.Run;
end.
