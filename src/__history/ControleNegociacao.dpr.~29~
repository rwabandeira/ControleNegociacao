program ControleNegociacao;

uses
  Forms,
  Menu in 'Fontes\Menu.pas' {FrMenu},
  _Conexao in 'DB\_DB.pas',
  CrudCadastro in 'Fontes\CrudCadastro.pas' {FrCrudCadastro},
