program ControleNegociacao;

uses
  Forms,
  Menu in 'Tela\Menu.pas' {FormMenu},
  _Conexao in 'Conexao\_Conexao.pas',
  CrudCadastro in 'Tela\CrudCadastro.pas' {FrCrudCadastro},
  Cadastros in 'Tela\Cadastros.pas' {FrCadastros},
  _Cadastro in 'Banco\_Cadastro.pas',
  PesquisarCadastros in 'Tela\PesquisarCadastros.pas' {FrPesquisarCadastros};

{$R *.res}

begin
  Application.Initialize;
  Application.MainFormOnTaskbar := True;
  Application.CreateForm(TFormMenu, FormMenu);
  Application.CreateForm(TFrCrudCadastro, FrCrudCadastro);
  Application.CreateForm(TFrCadastros, FrCadastros);
  Application.CreateForm(TFrPesquisarCadastros, FrPesquisarCadastros);
  Application.Run;
end.
